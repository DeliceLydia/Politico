{
  "name": "politico",
  "version": "1.0.0",
  "description": "POLITICO is a fullstack Javascript application that enables citizens to give their mandate to politicians running for different government offices while building trust in the process through transparency.",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon --exec babel-node ./server/server.js",
    "start": "npm run drop && npm run tables && nodemon --exec babel-node ./server/server.js",
    "tables": "node --require @babel/register ./server/database/database  createTables",
    "drop": "node --require @babel/register ./server/database/database dropTables",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeliceLydia/Politico.git"
  },
  "author": "Delice Lydia",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/DeliceLydia/Politico/issues"
  },
  "homepage": "https://github.com/DeliceLydia/Politico#readme",
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.7",
    "@babel/register": "^7.8.6",
    "bcrypt": "^4.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "nodemon": "^2.0.2",
    "pg": "^7.18.2"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1"
  }
}
